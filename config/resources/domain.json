{
  "version": "0.1",
  "prefixes": {
    "lblodmow": "http://data.lblod.info/vocabularies/mobiliteit/",
    "skos": "http://www.w3.org/2004/02/skos/core#",
    "org": "http://www.w3.org/ns/org#",
    "mobiliteit": "https://data.vlaanderen.be/ns/mobiliteit#",
    "vs": "http://www.w3.org/2003/06/sw-vocab-status/ns#",
    "ext": "http://mu.semte.ch/vocabularies/ext/"
  },
  "resources": {
    "road-sign-concepts": {
      "name": "roadSignConcept",
      "class": "mobiliteit:Verkeersbordconcept",
      "attributes": {
        "image": {
          "type": "url",
          "predicate": "mobiliteit:grafischeWeergave"
        },
        "meaning": {
          "type": "string",
          "predicate": "skos:scopeNote"
        },
        "road-sign-concept-code": {
          "type": "string",
          "predicate": "skos:prefLabel"
        }
      },
      "relationships": {
        "status": {
          "predicate": "vs:term_status",
          "target": "roadSignConceptStatus",
          "cardinality": "one"
        },
        "categories": {
          "predicate": "org:classification",
          "target": "roadSignCategory",
          "cardinality": "many"
        },
        "measures": {
          "predicate": "ext:measures",
          "target": "roadMeasure",
          "cardinality": "many"
        },
        "subSigns": {
          "predicate": "lblodmow:heeftOnderbordConcept",
          "target": "roadSignConcept",
          "cardinality": "many"
        },
        "mainSigns": {
          "predicate": "lblodmow:heeftOnderbordConcept",
          "target": "roadSignConcept",
          "cardinality": "many",
          "inverse": true
        },
        "relatedRoadSignConcepts": {
          "predicate": "lblodmow:heeftGerelateerdVerkeersbordconcept",
          "target": "roadSignConcept",
          "cardinality": "many"
        }
      },
      "new-resource-base": "http://data.lblod.info/verkeersbordconcepten/"
    },
    "road-marking-concepts": {
      "name": "roadMarkingConcept",
      "class": "mobiliteit:Wegmarkeringconcept",
      "attributes": {
        "image": {
          "type": "url",
          "predicate": "mobiliteit:grafischeWeergave"
        },
        "meaning": {
          "type": "string",
          "predicate": "skos:scopeNote"
        },
        "definition": {
          "type": "string",
          "predicate": "skos:definition"
        },
        "road-marking-concept-code": {
          "type": "string",
          "predicate": "skos:prefLabel"
        }
      },
      "relationships": {
        "status": {
          "predicate": "vs:term_status",
          "target": "roadMarkingConceptStatus",
          "cardinality": "one"
        },
        "relatedRoadSignConcepts": {
          "predicate": "lblodmow:heeftGerelateerdVerkeersbordconcept",
          "target": "roadSignConcept",
          "cardinality": "many"
        },
        "relatedRoadMarkingConcepts": {
          "predicate": "lblodmow:heeftGerelateerdWegmarkeringconcept",
          "target": "roadMarkingConcept",
          "cardinality": "many"
        }
      },
      "new-resource-base": "http://data.lblod.info/Wegmarkeringconcepten/"
    },
    "traffic-light-concepts": {
      "name": "trafficLightConcept",
      "class": "mobiliteit:Verkeerslichtconcept",
      "attributes": {
        "image": {
          "type": "url",
          "predicate": "mobiliteit:grafischeWeergave"
        },
        "meaning": {
          "type": "string",
          "predicate": "skos:scopeNote"
        },
        "definition": {
          "type": "string",
          "predicate": "skos:definition"
        },
        "traffic-light-concept-code": {
          "type": "string",
          "predicate": "skos:prefLabel"
        }
      },
      "relationships": {
        "status": {
          "predicate": "vs:term_status",
          "target": "trafficLightConceptStatus",
          "cardinality": "one"
        },
        "categories": {
          "predicate": "org:classification",
          "target": "trafficLightCategory",
          "cardinality": "many"
        },
        "relatedTrafficLightConcepts": {
          "predicate": "lblodmow:heeftGerelateerdVerkeerslichtconcept",
          "target": "trafficLightConcept",
          "cardinality": "many"
        }
      },
      "new-resource-base": "http://data.lblod.info/Verkeerslichtconcepten/"
    },
    "road-sign-categories": {
      "name": "roadSignCategory",
      "class": "mobiliteit:Verkeersbordcategorie",
      "attributes": {
        "label": {
          "type": "string",
          "predicate": "skos:prefLabel"
        }
      },
      "relationships": {
        "roadSignConcepts": {
          "predicate": "org:classification",
          "target": "roadSignConcept",
          "cardinality": "many",
          "inverse": true
        }
      },
      "new-resource-base": "http://data.lblod.info/verkeersbordcategorieen/"
    },
    "road-measures":{
      "name": "roadMeasure",
      "class": "ext:roadMeasure",
      "attributes": {
        "label": {
          "type": "string",
          "predicate": "skos:prefLabel"
        }
      },
      "relationships": {
        "roadSignConcepts": {
          "predicate": "ext:measures",
          "target": "roadSignConcept",
          "cardinality": "many",
          "inverse": true
        },
        "roadMeasureSections": {
          "predicate": "ext:measureSections",
          "target": "roadMeasureSection",
          "cardinality": "many",
          "inverse": true
        }
      },
      "new-resource-base": "http://data.lblod.info/RoadMeasures/"
    },
    "road-measure-variables":{
      "name": "roadMeasureVariable",
      "class": "ext:roadMeasureVariable",
      "attributes": {
        "label": {
          "type": "string",
          "predicate": "skos:prefLabel"
        },
        "type": {
          "type": "string",
          "predicate": "ext:variableType"
        }
      },
      "relationships":{
        "roadMeasureSection": {
          "predicate": "ext:measureVars",
          "target": "roadMeasureSection",
          "cardinality": "one"
        }
      },
      "new-resource-base": "http://data.lblod.info/RoadMeasureVariables/"
    },
    "road-measure-sections":{
      "name": "roadMeasureSection",
      "class": "ext:roadMeasureSection",
      "attributes": {
        "template": {
          "type": "string",
          "predicate": "ext:templateText"
        }
      },
      "relationships":{
        "roadMeasure": {
          "predicate": "ext:measureSections",
          "target": "roadMeasure",
          "cardinality": "one"
        },
        "variables": {
          "predicate": "ext:measureVars",
          "target": "roadMeasureVariable",
          "cardinality": "many",
          "inverse": true
        }
      },
      "new-resource-base": "http://data.lblod.info/RoadMeasureSections/"
    },
    "road-sign-concept-status": {
      "name": "roadSignConceptStatus",
      "class": "mobiliteit:VerkeersbordconceptStatus",
      "relationships": {
        "roadSignConceptStatusCode": {
          "predicate": "mobiliteit:VerkeersbordconceptStatus.status",
          "target": "roadSignConceptStatusCode",
          "cardinality": "one"
        },
        "roadSignConcept": {
          "predicate": "vs:term_status",
          "target": "roadSignConcept",
          "cardinality": "many",
          "inverse": true
        }
      },
      "new-resource-base": "http://data.lblod.info/verkeersbordconcept-status/"
    },
    "road-sign-concept-status-codes": {
      "name": "roadSignConceptStatusCode",
      "class": "lblodmow:VerkeersbordconceptStatusCode",
      "attributes": {
        "label": {
          "type": "string",
          "predicate": "skos:prefLabel"
        }
      },
      "relationships": {
        "roadSignConceptStatus": {
          "predicate": "mobiliteit:VerkeersbordconceptStatus.status",
          "target": "roadSignConceptStatus",
          "cardinality": "many",
          "inverse": true
        }
      },
      "new-resource-base": "http://data.lblod.info/verkeersbordconcept-status-codes/"
    }
  }
}
